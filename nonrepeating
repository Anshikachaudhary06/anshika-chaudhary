#include<iostream>
using namespace std;
class rep{
    public:
    string s;

void input(){
    cout<<"enter the string ";
    getline(cin,s);
}
void display(){
int c;
for(int i=0;s[i]!='\0';i++){
        c=0;
        for(int j=0;s[j]!='\0';j++){
            if(s[i]==s[j]&& s[i]!=' ')
            c++;
        }
    

    if(c==1){
        cout<<s[i]<<" ";
    }
    }
}
};
int main(){
rep d;
d.input();
d.display();
}
